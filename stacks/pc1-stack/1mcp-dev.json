{
  "mcpServers": {
    "docker": {
      "command": "docker-mcp-stdio",
      "tags": ["docker"],
      "connectionTimeout": 120000,
      "requestTimeout": 120000,
      "restartOnExit": true,
      "restartDelay": 2000
    },
    "mcp-agents": {
      "transport": "streamableHttp",
      "url": "http://mcp-agents:8046/mcp",
      "tags": ["pc1", "agents", "dev"],
      "enabled": true
    },
    "mcp-task": {
      "transport": "streamableHttp",
      "url": "http://mcp-task:8016/mcp",
      "tags": ["pc1", "task", "dev"],
      "enabled": true
    },
    "mcp-cuda": {
      "transport": "streamableHttp",
      "url": "http://pc1.vpn:8057/mcp",
      "tags": ["pc1", "cuda", "dev"],
      "enabled": true
    },
    "mcp-tester": {
      "transport": "streamableHttp",
      "url": "http://mcp-tester:8335/mcp",
      "tags": ["pc1", "tester", "dev"],
      "enabled": true
    },
    "mcp-http": {
      "transport": "streamableHttp",
      "url": "http://mcp-http:8067/mcp",
      "tags": ["pc1", "http", "dev"],
      "enabled": true
    },
    "mcp-rag": {
      "transport": "streamableHttp",
      "url": "http://pc1.vpn:8055/mcp",
      "tags": ["pc1", "rag", "dev"],
      "enabled": false
    },
    "mcp-doc-archiver": {
      "transport": "streamableHttp",
      "url": "http://pc1.vpn:8066/mcp",
      "tags": ["pc1", "doc-archiver", "dev"],
      "enabled": true
    },
    "mcp-deka": {
      "transport": "streamableHttp",
      "url": "http://pc1.vpn:8270/mcp",
      "tags": ["pc1", "deka", "dev"],
      "enabled": true
    },
    "mcp-imagen-light": {
      "transport": "streamableHttp",
      "url": "http://pc1.vpn:8020/mcp",
      "tags": ["pc1", "imagen", "sdxl", "dev"],
      "enabled": true
    },
    "mcp-project-manager": {
      "command": "npx",
      "args": ["-y", "mcp-project-manager"],
      "env": {
        "MCP_TASK_DATA_DIR": "/data/pm"
      },
      "tags": ["pc1", "project", "pm", "dev"],
      "connectionTimeout": 60000,
      "requestTimeout": 60000,
      "restartOnExit": true,
      "restartDelay": 1000,
      "enabled": true
    }
  }
}

{
  "mcpServers": {
    "docker": {
      "command": "docker-mcp-stdio",
      "tags": ["docker"],
      "connectionTimeout": 120000,
      "requestTimeout": 120000,
      "restartOnExit": true,
      "restartDelay": 2000
    },
    "mcp-agents": {
      "transportType": "http",
      "url": "http://mcp-agents:8046/mcp",
      "tags": ["pc1", "agents"],
      "enabled": true
    },

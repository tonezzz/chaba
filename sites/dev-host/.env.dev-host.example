# Dev-host local configuration template
# Copy this file to .env.dev-host and adjust values as needed.

# Base URL used when rendering the landing page links
DEV_HOST_BASE_URL=http://dev-host:3100

# Optional token required to call /api/deploy/* endpoints
DEV_HOST_PUBLISH_TOKEN=

# Downstream proxy targets (override as needed)
GLAMA_PROXY_TARGET=http://127.0.0.1:4020
AGENTS_PROXY_TARGET=http://127.0.0.1:4060
DETECTS_PROXY_TARGET=http://localhost:4120

# 1mcp target (used for Imagen tool invocation)
ONE_MCP_BASE_URL=http://1mcp.pc2.vpn:3050
ONE_MCP_APP=windsurf
IMAGEN_MCP_TOOL_NAME=mcp-imagen_1mcp_generate_image
